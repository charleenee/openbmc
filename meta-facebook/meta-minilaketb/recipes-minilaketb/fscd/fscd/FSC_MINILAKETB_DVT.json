{
  "version": "minilaketb-dvt-r02",
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "sample_interval_ms": 3000,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "sensor_valid_check": {
    "soc": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "nvme": {
      "attribute": {
        "type" : "power_status"
      }
    }
  },
  "watchdog": false,
  "min_rpm": 800,
  "profiles": {
    "linear_inlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [ 20, 25 ],
        [ 25, 28 ],
        [ 29, 29 ],
        [ 30, 30 ],
        [ 33, 35 ],
        [ 35, 40 ],
        [ 38, 50 ],
        [ 41, 60 ],
        [ 43, 65 ],
        [ 45, 65 ],
        [ 48, 100 ]
      ]
    },
    "linear_cpu_margin": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [ -17, 25 ],
        [ -13, 45 ],
        [ -9, 60 ],
        [ -7, 65 ],
        [ -5, 100 ]
      ]
    },
    "linear_dimm": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [70, 20],
        [73, 35],
        [75, 50],
        [80, 65]
      ]
    }
  },
  "fans": {
    "0": {
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 0"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 0"
      }
    },
    "1": {
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 1"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 1"
      }
    }
  },
  "zones": {
    "zone_1": {
      "pwm_output": [0, 1],
      "expr_file": "FSC_MINILAKETB_DVT.fsc"
    }
  }
}
